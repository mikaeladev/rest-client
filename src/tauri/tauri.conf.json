{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "rest-client",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../../dist",
    "devUrl": "http://localhost:7378",
    "beforeDevCommand": "bun run dev:vite",
    "beforeBuildCommand": "bun run build:vite"
  },
  "app": {
    "windows": [
      {
        "title": "rest-client",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
